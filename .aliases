# Shell
alias ll='ls -alF --group-directories-first'
alias lld='ll -d'
alias rm='rm -iv'
alias cp='cp -iv'
alias mv='mv -iv'
alias mkdir='mkdir -pv'
alias shh='echo "No, YOU be quiet!" #'

# Python virtualenv
alias pyve='source env/bin/activate'

# Git
alias gitap='git add -p'
alias gitch='git checkout'
alias gitchb='gitch -b'
alias gitchd='gitch develop'
alias gitchm='gitch master'
alias gitco='git commit'
alias gitcoa='gitco --amend'
alias gitcom='gitco -m "mblakesley was here"'
alias gitd='git diff'
alias gitdc='gitd --cached'
alias gitdh1='gitd HEAD~1'
alias gitdh2='gitd HEAD~2'
alias gitdh3='gitd HEAD~3'
alias gitfp='git fetch -p'
alias gitl='git log'
alias gitlo='git log --oneline --graph'
alias gitps='git push'
alias gitpsf='gitps -f'
alias gitpsu='gitps -u origin'
alias gitreb='git rebase'
alias gitrebi='gitreb -i'
alias gitrebd='gitrebi develop'
alias gitrebm='gitrebi master'
alias gitreba='gitreb --abort'
alias gitrebc='gitreb --continue'
alias gitrebh1='gitrebi HEAD~1'
alias gitrebh2='gitrebi HEAD~2'
alias gitrebh3='gitrebi HEAD~3'
alias gitrebh4='gitrebi HEAD~4'
alias gitrebh5='gitrebi HEAD~5'
alias gitrebh6='gitrebi HEAD~6'
alias gitrebh7='gitrebi HEAD~7'
alias gitrebh8='gitrebi HEAD~8'
alias gitrebh9='gitrebi HEAD~9'
alias gitres='git reset'
alias gitresha='gitres --hard'
alias gitresh1='gitres HEAD~1'
alias gits='git branch -a; git status'
alias gitup='gitfp; git pull'
alias gitupd='gitchd; gitup'
alias gitupm='gitchm; gitup'

