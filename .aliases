# Linux
alias ll='ls -alF --group-directories-first'
alias lld='ll -d'
alias rm='rm -iv'
alias cp='cp -iv'
alias mv='mv -iv'
alias shh='echo "No, YOU be quiet!" #'

# Python virtualenv
alias pyve='source env/bin/activate'

# Git
alias gitap='git add -p'
alias gitchd='git checkout develop'
alias gitchm='git checkout master'
alias gitco='git commit'
alias gitcoa='gitco --amend'
alias gitcom='gitco -m "mblakesley was here"'
alias gitd='git diff'
alias gitdc='gitd --cached'
alias gitdh1='gitd HEAD~1'
alias gitfp='git fetch -p'
alias gitl='git log'
alias gitlo='git log --oneline --graph'
alias gitpu='git push'
alias gitpuf='gitpu -f'
alias gitrebd='git rebase -i develop'
alias gitrebm='git rebase -i master'
alias gitreba='git rebase --abort'
alias gitrebc='git rebase --continue'
alias gitrebh1='git rebase -i HEAD~1'
alias gitrebh2='git rebase -i HEAD~2'
alias gitrebh3='git rebase -i HEAD~3'
alias gitrebh4='git rebase -i HEAD~4'
alias gitrebh5='git rebase -i HEAD~5'
alias gitrebh6='git rebase -i HEAD~6'
alias gitrebh7='git rebase -i HEAD~7'
alias gitrebh8='git rebase -i HEAD~8'
alias gitrebh9='git rebase -i HEAD~9'
alias gitres='git reset'
alias gitresha='gitres --hard'
alias gitresh1='gitres HEAD~1'
alias gits='git branch -a; git status'
alias gitup='gitfp; git pull'
alias gitupd='gitchd; gitup'
alias gitupm='gitchm; gitup'

